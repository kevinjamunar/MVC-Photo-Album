<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane prefHeight="230.0" prefWidth="438.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.renameAlbumController">
   <children>
      <Text layoutX="36.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="What do you want to rename this album to?" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="renameTextField" layoutX="109.0" layoutY="82.0" prefHeight="27.0" prefWidth="220.0" promptText="Album Name" />
      <Button fx:id="submitRenameButton" layoutX="149.0" layoutY="123.0" mnemonicParsing="false" text="Submit" />
      <Button fx:id="cancelRenameButton" layoutX="227.0" layoutY="123.0" mnemonicParsing="false" text="Cancel" />
      <TextFlow layoutX="99.0" layoutY="168.0" prefHeight="34.0" prefWidth="247.0" textAlignment="CENTER">
         <children>
            <Text fx:id="errorMessageText" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Text>
         </children>
      </TextFlow>
   </children>
</AnchorPane>
